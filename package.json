{
	"name": "musicion",
	"version": "0.1.0",
	"description": "Multi Platform Music Player",
	"main": "main.js",
	"scripts": {
		"start": "webpack-dev-server --config ./webpack/browser-dev.js",
		"es": "webpack-dev-server --config ./webpack/electron-dev.js",
		"ed": "electron .",
		"ebuild": "rm -rf renderer && webpack --config ./webpack/electron-prod.js",
		"esubuild": "node ./build/soft-update.js",
		"electron": "webpack --config ./webpack/prod.js && electron .",
		"pcreate": "phonegap create phonegap --id 'com.codefoxes.musicion' --name 'musicion'",
		"ps": "rm -rf phonegap/www/* && webpack --watch --config ./webpack/cordova-dev.js",
		"pd": "cd phonegap && phonegap serve",
		"test": "echo \"No test specified\" && exit 0",
		"dist": "electron-builder"
	},
	"keywords": [],
	"author": "Codefoxes",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/preset-env": "^7.4.3",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"copy-webpack-plugin": "^5.0.3",
		"css-hot-loader": "^1.4.4",
		"css-loader": "^2.1.1",
		"electron": "^5.0.2",
		"electron-builder": "^20.41.0",
		"electron-reload": "^1.4.0",
		"eslint-config-airbnb-standard": "^3.0.1",
		"file-loader": "^3.0.1",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.6.0",
		"node-sass": "^4.12.0",
		"react-hot-loader": "^4.8.4",
		"sass-loader": "^7.1.0",
		"shelljs": "^0.8.3",
		"url-loader": "^1.1.2",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.1",
		"webpack-dev-server": "^3.3.1",
		"webpack-merge": "^4.2.1"
	},
	"dependencies": {
		"adm-zip": "^0.4.13",
		"dirsum": "^0.1.1",
		"fsevents": "^2.0.7",
		"graceful-fs": "^4.1.15",
		"howler": "^2.1.2",
		"node-id3": "^0.1.7",
		"prop-types": "^15.7.2",
		"rc-slider": "^8.6.11",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-split-pane": "^0.1.87",
		"react-table": "^6.10.0"
	},
	"build": {
		"appId": "com.codefoxes.musicion",
		"compression": "maximum",
		"dmg": {
			"contents": [
				{
					"x": 410,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				},
				{
					"x": 130,
					"y": 150,
					"type": "file"
				}
			]
		},
		"linux": {
			"target": "deb",
			"maintainer": "dev@codefoxes.com"
		}
	}
}
